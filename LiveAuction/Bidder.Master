        function LoginValidate() {
            if (RequiredTxt('txtLoginEmail', 'Email required')) {
                return false;
            }
            if (ValEMail('txtLoginEmail', 'Invalid  Email')) {
                return false;
            }
            if (RequiredTxt('txtLoginPassword', 'Password required')) {
                return false;
            }
            return true;
        }
    </script>
